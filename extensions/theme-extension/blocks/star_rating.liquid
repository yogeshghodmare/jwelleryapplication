          "https://cdn.shopify.com/s/files/1/0631/6412/1280/files/Center_Diamond_he.png",
          "https://cdn.shopify.com/s/files/1/0631/6412/1280/files/Center_Diamond_ov.png",
          "https://cdn.shopify.com/s/files/1/0631/6412/1280/files/Center_Diamond_ra.png"
      ];
  
      var productid = {{ product.id }};
  
      fetch(`https://skyvisionshopify.in/KattDiamondApi/${productid}`)
          .then(response => response.json())
          .then(data => {
              var varry1 = [];
              varry1.push(data.data);
              var varry = varry1[0];
  
              var ans = [];
            
  
              varry.map((variant) => {
                  if (!ans.includes(variant.ShapeId)) {
                      ans.push(variant.ShapeId);
                  }
              });
  
              ans.sort();
             
              ans.map((id) => {
                  var elem = document.createElement('img');
                  elem.setAttribute('src', shapeidArray[id]);
                  elem.setAttribute('height', "{{image.height}}");
                  elem.setAttribute('width', "{{image.width}}");
                  elem.setAttribute('class', "img");
                  elem.addEventListener('click', function() {
                      makeActive1(id); 
                  });
                  
                    var header = document.getElementById("centerstoneimages");
                    var btns = header.getElementsByClassName("img");
                    for (var i = 0; i < btns.length; i++) {
                      btns[i].addEventListener("click", function() {
                      var current = document.getElementsByClassName("active");
                      current[0].className = current[0].className.replace(" active", "");
                      this.className += " active";
                      });
                    }
                 
  
                  document.getElementById("centerstoneimages").appendChild(elem);
              });
          })
          .catch(error => {
              console.error('Error fetching data:', error);
          });

  });
  
