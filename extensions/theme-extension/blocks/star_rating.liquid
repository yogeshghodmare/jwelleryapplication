  <script>
  
    // Code for Send product with options in Add to cart page
    async function cartChange() {
     
      
      const apiUrl = 'https://skyvisionshopify.in/KattDiamondApi/createproduct';
  
      var weight=document.getElementById('WeightRangeValue').value
      var color=document.getElementById('SecondSlider').value
      var clarity=document.getElementById('ThirdSlider').value
  
    var pid;
    var productname = document.getElementById("productname").innerText;
    var carat = document.getElementById("carat").innerText;
    var productName = productname+" "+carat;
    console.log(productName);
  
       var pricevalueElement = document.getElementById("pricevalue");
       var priceWithCurrency = pricevalueElement.innerText; 
       
  const substring = priceWithCurrency.replace("Rs. ", "");
      
       var price = parseFloat(substring.replace(/,/g, "")) 
       console.log("Price:", substring);
       
       
      try {
        const response = await fetch(apiUrl, {
            method: 'POST',
            headers: { 'Content-Type': 'application/json' },
            body: JSON.stringify({
                "title": productName,
                "body_html": "<strong>Good snowboard!</strong>",
                "variants": [
                  {
                      
                      "price": price
                      
                  }
              ]
            }),
        });
  
        const data = await response.json();
        console.log("new product ", data);
        console.log("id",data.product.variants[0].id)
        pid=data.product.variants[0].id
        
    } catch (error) {
        console.error('Error:', error);
    }
      
    var metaltitle=document.getElementById("variable-mounting").innerText
    var data = {
      items: [
        {
          id: pid,
          quantity: 1,// Corrected spelling
          properties: {
            Center_Stone_Weight : weight,
            Center_Stone_Color:  color,
            Center_Stone_Clarity:  clarity, 
            Metal_for_Mounting: metaltitle
            ,
            Ring_Size: "3.00"
        }
        }
      ]
    };
    
        
          try {
            const response = await fetch("/cart/add.json", {
              method: "POST",
              headers: {
                "Content-Type": "application/json",
                "Accept": "application/json"
              },
              body: JSON.stringify(data)
            });
        
            const result = await response.json();
            console.log("result", result);

            try{
              const Database = await fetch("https://skyvisionshopify.in/KattDiamondApi/add_cart", {
                method: "POST",
                headers: {
                  "Content-Type": "application/json",
                  "Accept": "application/json"
                },
                body: JSON.stringify({
                  "product_id": pid,
                  "product_title": productName,
                  
                
              })
              });
          
              const res = await Database.json();
              console.log("res", res);
            }
            catch(e){
            console.log("Data not send to the Database")
          }


            alert("Added to Cart")
            
            // Further actions with the result if needed
          } catch (error) {
            console.error('Error:', error);
            // Handle errors here if necessary
          }  
       
  }
  
